{
  "manifest_version": 3,
  "name": "Get YouTube",
  "version": "1.0",
  "description": "Get YouTube videos",
  "permissions": [
    "activeTab",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "getYouTube@tobias.com"
    }
  }
}
